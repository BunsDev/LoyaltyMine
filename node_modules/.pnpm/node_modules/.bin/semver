#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/Documents/GitHub/LoyaltyMine/node_modules/.pnpm/semver@6.3.0/node_modules/semver/bin/semver.js/node_modules:/Users/unico/Documents/GitHub/LoyaltyMine/node_modules/.pnpm/semver@6.3.0/node_modules/semver/bin/node_modules:/Users/unico/Documents/GitHub/LoyaltyMine/node_modules/.pnpm/semver@6.3.0/node_modules/semver/node_modules:/Users/unico/Documents/GitHub/LoyaltyMine/node_modules/.pnpm/semver@6.3.0/node_modules:/Users/unico/Documents/GitHub/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/Documents/GitHub/LoyaltyMine/node_modules:/Users/unico/Documents/GitHub/node_modules:/Users/unico/Documents/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/Documents/GitHub/LoyaltyMine/node_modules/.pnpm/node_modules/semver/bin/semver.js/node_modules:/Users/unico/Documents/GitHub/LoyaltyMine/node_modules/.pnpm/node_modules/semver/bin/node_modules:/Users/unico/Documents/GitHub/LoyaltyMine/node_modules/.pnpm/node_modules/semver/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver.js" "$@"
else 
  exec node  "$basedir/../semver/bin/semver.js" "$@"
fi
